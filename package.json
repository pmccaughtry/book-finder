{
    "dependencies": {
        "next": "15.0.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "concurrently": "^9.1.0",
        "eslint": "^8",
        "eslint-config-next": "15.0.4",
        "postcss": "^8",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    },
    "name": "book-finder",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
        "build": "next build",
        "fastapi-dev": "(cd api && pip3 install -r requirements.txt && fastapi dev main.py)",
        "next-dev": "next dev --turbopack",
        "start": "next start",
        "lint": "next lint"
    },
    "version": "0.1.0"
}
